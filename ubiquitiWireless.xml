<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2022-08-29T17:34:54Z</date>
    <groups>
        <group>
            <name>Templates-LSN</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Wireless_Ubiquit</template>
            <name>Wireless_Ubiquit</name>
            <groups>
                <group>
                    <name>Templates-LSN</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>★Wireless★</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>Discovery_Wireless_UBNT</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#IFDESCR},IF-MIB::ifDescr]</snmp_oid>
                    <key>ifDescr</key>
                    <delay>30m</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#IFDESCR}</macro>
                                <value>lo</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <lifetime>3d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Quantidade_clientes_{#IFDESCR}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>UBNT-AirMAX-MIB::ubntWlStatStaCount.{#SNMPINDEX}</snmp_oid>
                            <key>count_cliente_ubnt[UBNT-AirMAX-MIB::ubntWlStatStaCount.{#SNMPINDEX}]</key>
                            <delay>10m</delay>
                            <history>7d</history>
                            <trends>5d</trends>
                            <applications>
                                <application>
                                    <name>★Wireless★</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last(0)}&gt;20</expression>
                                    <name>Maximo de clientes conectados.</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{min(20m)}=0</expression>
                                    <name>Nenhum cliente conectado.</name>
                                    <priority>INFO</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Freqüência_{#IFDESCR}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>UBNT-AirMAX-MIB::ubntRadioFreq.{#SNMPINDEX}</snmp_oid>
                            <key>frequencia_ubnt[UBNT-AirMAX-MIB::ubntRadioFreq.{#SNMPINDEX}]</key>
                            <delay>10m</delay>
                            <history>7d</history>
                            <trends>5d</trends>
                            <applications>
                                <application>
                                    <name>★Wireless★</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{diff()}=1</expression>
                                    <name>Freqüência foi alterada.</name>
                                    <priority>INFO</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Country_{#IFDESCR}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>UBNT-AirMAX-MIB::ubntRadioCCode.{#SNMPINDEX}</snmp_oid>
                            <key>pais_ubnt[UBNT-AirMAX-MIB::ubntRadioCCode.{#SNMPINDEX}]</key>
                            <delay>120m</delay>
                            <trends>0</trends>
                            <applications>
                                <application>
                                    <name>★Wireless★</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>Country_(País)</name>
                            </valuemap>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{diff()}=1</expression>
                                    <name>Foi alterado país.</name>
                                    <priority>INFO</priority>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;511</expression>
                                    <name>No  CT.</name>
                                    <priority>INFO</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>SINAL_{#IFDESCR}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>UBNT-AirMAX-MIB::ubntWlStatSignal.{#SNMPINDEX}</snmp_oid>
                            <key>sinal_ubnt[UBNT-AirMAX-MIB::ubntWlStatSignal.{#SNMPINDEX}]</key>
                            <delay>10m</delay>
                            <history>7d</history>
                            <trends>5d</trends>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>★Wireless★</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{min(10m)}&gt;70</expression>
                                    <name>Sinal ruim.</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>SSID_{#IFDESCR}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>UBNT-AirMAX-MIB::ubntWlStatSsid.{#SNMPINDEX}</snmp_oid>
                            <key>ssid_ubnt[UBNT-AirMAX-MIB::ubntWlStatSsid.{#SNMPINDEX}]</key>
                            <delay>120m</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <applications>
                                <application>
                                    <name>★Wireless★</name>
                                </application>
                            </applications>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{diff()}=1</expression>
                                    <name>SSID foi alterado.</name>
                                    <priority>INFO</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
    <value_maps>
        <value_map>
            <name>Country_(País)</name>
            <mappings>
                <mapping>
                    <value>32</value>
                    <newvalue>Argentina</newvalue>
                </mapping>
                <mapping>
                    <value>76</value>
                    <newvalue>Brasil</newvalue>
                </mapping>
                <mapping>
                    <value>511</value>
                    <newvalue>Compilance_Test</newvalue>
                </mapping>
            </mappings>
        </value_map>
    </value_maps>
</zabbix_export>
